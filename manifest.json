{
    "name": "Poll Attender",
    "description": "Attend the google meet polls automatically",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html"
    },
    "commands":{
        "_execute_action": {
            "suggested_key": {
              "windows": "Ctrl+q",
              "mac": "Command+q",
              "chromeos": "Ctrl+q",
              "linux": "Ctrl+b"
            }
          }
    }
}